{
  "name": "ktan-cocktail",
  "version": "0.2.3",
  "description": "A bunch of reusable components in React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar src/styles -o src/styles",
    "build-js": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/src -o src/styles --watch --recursive"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "inferno": "^3.10.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "nwb": "0.21.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6"
  },
  "author": "Frederic MAMATH",
  "homepage": "",
  "license": "MIT",
  "repository": "https://github.com/FredericKtan/cocktail",
  "keywords": [
    "react-component"
  ]
}
